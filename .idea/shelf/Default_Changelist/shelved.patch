(\"#errors p\").innerText = JSON.stringify(err);\n            console.log(\"Client error: \" + err);\n        });\n    });\n\n    function showModal() {\n        $('#myModal').modal('show');\n    }\n\n</script>\n\n\n\n<script src=\"https://web.getpinch.com.au/capture/capture.bundle.js\"></script>\n</script>
